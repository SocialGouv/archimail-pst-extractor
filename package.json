    "@types/node": "^14",
    "cross-env": "^7.0.3",
    "eslint": "^7",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.2.5",
    "jest-junit": "^13.0.0",
    "npm-run-all": "^4.1.5",
